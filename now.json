{
  "version": 2,
  "name": "goodWork",
  "builds": [
    { "src": "server/now.js", "use": "@now/node" },
    { "src": "client/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/graphql", "dest": "/server/now.js" },
    { "src": "/(.*)", "dest": "/client/$1" }
  ],
  "env": {
    "NODE_ENV": "development"
  }
}
